{
  "name": "unit-testing-cheatsheet",
  "version": "1.0.0",
  "description": "Jest unit test examples",
  "keywords": [
    "jest"
  ],
  "main": "src/index.js",
  "homepage":"https://silverhub.github.io/unit-testing-cheatsheet/",
  "dependencies": {
    "msw": "^0.21.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "react-test-renderer": "^16.14.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
